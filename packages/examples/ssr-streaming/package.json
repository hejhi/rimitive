{
  "name": "@lattice/example-ssr-streaming",
  "version": "0.0.1",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "tsx watch src/server.ts",
    "build:server": "tsc --project tsconfig.server.json",
    "build:client": "vite build",
    "build": "pnpm build:server && pnpm build:client",
    "start": "node dist/server/server.js",
    "typecheck": "tsc --noEmit"
  },
  "dependencies": {
    "@lattice/lattice": "workspace:*",
    "@lattice/resource": "workspace:*",
    "@lattice/router": "workspace:*",
    "@lattice/signals": "workspace:*",
    "@lattice/ssr": "workspace:*",
    "@lattice/view": "workspace:*"
  },
  "devDependencies": {
    "@types/node": "25.0.1",
    "tsx": "4.21.0",
    "typescript": "5.9.3",
    "vite": "7.2.7"
  }
}
