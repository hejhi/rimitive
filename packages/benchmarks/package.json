{
  "name": "@lattice/benchmarks",
  "version": "0.0.1",
  "description": "Performance benchmarks for Lattice framework",
  "author": "Lattice Team",
  "license": "MIT",
  "type": "module",
  "private": true,
  "scripts": {
    "build": "pnpm --filter @lattice/signals check && pnpm --filter @lattice/signals build",
    "bench": "tsx --expose-gc src/runner.ts",
    "typecheck": "tsc --noEmit",
    "lint": "eslint . --ext .ts,.tsx",
    "check": "pnpm typecheck && pnpm lint"
  },
  "devDependencies": {
    "@lattice/lattice": "workspace:*",
    "@lattice/signals": "workspace:*",
    "@preact/signals-core": "^1.8.0",
    "@reduxjs/toolkit": "^2.8.2",
    "@testing-library/react": "^16.3.0",
    "@types/node": "^20.0.0",
    "@types/react": "^19.2.2",
    "@types/react-dom": "^19.1.6",
    "glob": "^11.0.3",
    "jsdom": "^27.1.0",
    "mitata": "^1.0.34",
    "mobx": "^6.13.6",
    "pinia": "^3.0.3",
    "react": "^19.2.0",
    "react-dom": "^19.2.0",
    "terser": "^5.36.0",
    "tsx": "^4.7.0",
    "vite": "^7.1.1",
    "vitest": "^4.0.6",
    "vue": "^3.5.0",
    "zustand": "^5.0.3"
  },
  "dependencies": {
    "alien-signals": "^3.0.5"
  }
}