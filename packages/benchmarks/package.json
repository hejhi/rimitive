{
  "name": "@lattice/benchmarks",
  "version": "0.0.1",
  "description": "Performance benchmarks for Lattice framework",
  "author": "Lattice Team",
  "license": "MIT",
  "type": "module",
  "private": true,
  "scripts": {
    "build": "pnpm --filter @lattice/signals build",
    "bench": "tsx --expose-gc src/runner.ts",
    "typecheck": "tsc --noEmit",
    "lint": "eslint . --ext .ts,.tsx",
    "check": "pnpm typecheck && pnpm lint"
  },
  "devDependencies": {
    "@lattice/lattice": "workspace:*",
    "@lattice/signals": "workspace:*",
    "@preact/signals-core": "1.12.1",
    "@reduxjs/toolkit": "2.11.1",
    "@testing-library/react": "16.3.0",
    "@types/node": "25.0.1",
    "@types/react": "19.2.7",
    "@types/react-dom": "19.2.3",
    "glob": "13.0.0",
    "jsdom": "27.3.0",
    "mitata": "1.0.34",
    "mobx": "6.15.0",
    "pinia": "3.0.4",
    "react": "19.2.3",
    "react-dom": "19.2.3",
    "terser": "5.44.1",
    "tsx": "4.21.0",
    "vite": "7.2.7",
    "vitest": "4.0.15",
    "vue": "3.5.25"
  },
  "dependencies": {
    "alien-signals": "3.1.1"
  }
}
