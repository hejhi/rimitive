{
  "name": "@lattice/benchmarks",
  "version": "0.0.1",
  "description": "Performance benchmarks for Lattice framework",
  "author": "Lattice Team",
  "license": "MIT",
  "type": "module",
  "private": true,
  "scripts": {
    "bench": "pnpm bench:report:real",
    "bench:store-react": "node --expose-gc scripts/run-chunked-benchmarks.js real store-react",
    "bench:lattice": "node --expose-gc scripts/run-chunked-benchmarks.js real lattice",
    "bench:report:raw": "LATTICE_DISABLE_MEMOIZATION=true node --expose-gc scripts/run-chunked-benchmarks.js raw",
    "bench:report:real": "node --expose-gc scripts/run-chunked-benchmarks.js real",
    "bench:bundle": "node scripts/analyze-bundle-size.js",
    "bench:all": "pnpm bench:bundle && pnpm bench",
    "dashboard": "vite --config vite.config.dashboard.ts",
    "typecheck": "tsc --noEmit",
    "check": "pnpm typecheck"
  },
  "devDependencies": {
    "@lattice/adapter-redux": "workspace:*",
    "@lattice/adapter-store-react": "workspace:*",
    "@lattice/adapter-zustand": "workspace:*",
    "@lattice/core": "workspace:*",
    "@lattice/frameworks": "workspace:*",
    "@reduxjs/toolkit": "^2.8.2",
    "@sveltejs/vite-plugin-svelte": "^5.1.0",
    "@testing-library/react": "^14.0.0",
    "@types/node": "^20.0.0",
    "@types/react": "^18.3.0",
    "@types/react-dom": "^19.1.6",
    "jsdom": "^25.0.0",
    "mobx": "^6.13.6",
    "pinia": "^2.3.0",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "svelte": "^5.0.0",
    "terser": "^5.36.0",
    "tsx": "^4.7.0",
    "vite": "^6.0.0",
    "vitest": "^3.1.1",
    "vue": "^3.5.0",
    "zustand": "^5.0.3"
  }
}
