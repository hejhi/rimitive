{
  "name": "@rimitive/benchmarks",
  "version": "0.0.1",
  "description": "Performance benchmarks for Rimitive framework",
  "author": "Rimitive Team",
  "license": "MIT",
  "type": "module",
  "private": true,
  "scripts": {
    "build": "pnpm --filter @rimitive/signals build && pnpm --filter @rimitive/view build",
    "bench": "tsx --expose-gc src/runner.ts",
    "bench:docs": "tsx --expose-gc src/runner.ts --docs",
    "typecheck": "tsc --noEmit",
    "lint": "eslint . --ext .ts,.tsx",
    "check": "pnpm typecheck && pnpm lint"
  },
  "devDependencies": {
    "@rimitive/core": "workspace:*",
    "@rimitive/signals": "workspace:*",
    "@rimitive/view": "workspace:*",
    "@preact/signals-core": "1.12.1",
    "@reduxjs/toolkit": "2.11.2",
    "@testing-library/react": "16.3.2",
    "@types/node": "25.2.0",
    "@types/react": "19.2.8",
    "@types/react-dom": "19.2.3",
    "glob": "13.0.0",
    "jsdom": "27.4.0",
    "mitata": "1.0.34",
    "mobx": "6.15.0",
    "pinia": "3.0.4",
    "react": "19.2.3",
    "react-dom": "19.2.3",
    "terser": "5.46.0",
    "tsx": "4.21.0",
    "vite": "7.3.1",
    "vitest": "4.0.17",
    "vue": "3.5.27"
  },
  "dependencies": {
    "alien-signals": "3.1.2"
  }
}
