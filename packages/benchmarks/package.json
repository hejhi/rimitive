{
  "name": "@lattice/benchmarks",
  "version": "0.0.1",
  "description": "Performance benchmarks for Lattice framework",
  "author": "Lattice Team",
  "license": "MIT",
  "type": "module",
  "private": true,
  "scripts": {
    "bench": "pnpm bench:raw && pnpm bench:real",
    "bench:raw": "LATTICE_DISABLE_MEMOIZATION=true NODE_OPTIONS=--max-old-space-size=4096 vitest bench --run",
    "bench:real": "NODE_OPTIONS=--max-old-space-size=4096 vitest bench --run",
    "bench:report:raw": "LATTICE_DISABLE_MEMOIZATION=true node scripts/run-chunked-benchmarks.js raw",
    "bench:report:real": "node scripts/run-chunked-benchmarks.js real",
    "bench:merge": "node scripts/merge-benchmark-results.js",
    "bench:split": "node scripts/split-benchmark-results.js",
    "typecheck": "tsc --noEmit",
    "check": "pnpm typecheck"
  },
  "devDependencies": {
    "@lattice/adapter-pinia": "workspace:*",
    "@lattice/adapter-redux": "workspace:*",
    "@lattice/adapter-store-react": "workspace:*",
    "@lattice/adapter-zustand": "workspace:*",
    "@lattice/core": "workspace:*",
    "@lattice/runtime": "workspace:*",
    "@lattice/store-react": "workspace:*",
    "@lattice/store-vue": "workspace:*",
    "@reduxjs/toolkit": "^2.8.2",
    "@testing-library/react": "^14.0.0",
    "@types/node": "^20.0.0",
    "@types/react": "^18.3.0",
    "@types/react-dom": "^19.1.6",
    "jsdom": "^25.0.0",
    "pinia": "^2.3.0",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "vitest": "^3.1.1",
    "vue": "^3.5.0",
    "zustand": "^5.0.3"
  }
}
