{
  "name": "@lattice/benchmarks",
  "version": "0.0.1",
  "description": "Performance benchmarks for Lattice framework",
  "author": "Lattice Team",
  "license": "MIT",
  "type": "module",
  "private": true,
  "scripts": {
    "bench": "vitest bench --run",
    "bench:ci": "vitest bench --reporter=default --reporter=json --outputFile=bench-results.json",
    "bench:compare": "node scripts/compare-benchmarks.js",
    "typecheck": "tsc --noEmit",
    "check": "pnpm typecheck"
  },
  "devDependencies": {
    "@lattice/core": "workspace:*",
    "@lattice/adapter-zustand": "workspace:*",
    "@lattice/adapter-redux": "workspace:*",
    "@lattice/adapter-pinia": "workspace:*",
    "@lattice/store-react": "workspace:*",
    "@lattice/store-vue": "workspace:*",
    "@types/node": "^20.0.0",
    "vitest": "^3.1.1",
    "zustand": "^5.0.3",
    "@reduxjs/toolkit": "^2.8.2",
    "pinia": "^2.3.0",
    "vue": "^3.5.0",
    "react": "^18.3.1",
    "@testing-library/react": "^14.0.0"
  }
}
