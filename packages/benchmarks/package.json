{
  "name": "@lattice/benchmarks",
  "version": "0.0.1",
  "description": "Performance benchmarks for Lattice framework",
  "author": "Lattice Team",
  "license": "MIT",
  "type": "module",
  "private": true,
  "scripts": {
    "build:all": "cd ../ && pnpm build && cd benchmarks",
    "bench": "pnpm build:all && node --expose-gc scripts/run-chunked-benchmarks.js real",
    "bench:lattice": "node --expose-gc scripts/run-chunked-benchmarks.js real lattice",
    "bench:report:real": "node --expose-gc scripts/run-chunked-benchmarks.js real",
    "typecheck": "tsc --noEmit",
    "lint": "eslint . --ext .ts,.tsx",
    "check": "pnpm typecheck && pnpm lint"
  },
  "devDependencies": {
    "@lattice/lattice": "workspace:*",
    "@lattice/signals": "workspace:*",
    "@preact/signals-core": "^1.8.0",
    "@reduxjs/toolkit": "^2.8.2",
    "@testing-library/react": "^14.0.0",
    "@types/node": "^24.0.15",
    "@types/react": "^18.3.0",
    "@types/react-dom": "^19.1.6",
    "glob": "^11.0.3",
    "jsdom": "^25.0.0",
    "mobx": "^6.13.6",
    "pinia": "^2.3.0",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "terser": "^5.36.0",
    "tsx": "^4.7.0",
    "vite": "^6.0.0",
    "vitest": "^3.1.1",
    "vue": "^3.5.0",
    "zustand": "^5.0.3"
  }
}
