{
  "name": "@lattice/benchmarks",
  "version": "0.0.1",
  "description": "Performance benchmarks for Lattice framework",
  "author": "Lattice Team",
  "license": "MIT",
  "type": "module",
  "private": true,
  "scripts": {
    "build:all": "cd ../ && pnpm build && cd benchmarks",
    "bench": "pnpm build:all && tsx --expose-gc src/runner.ts",
    "bench:all": "pnpm build:all && tsx --expose-gc src/runner.ts",
    "bench:quick": "tsx --expose-gc src/runner.ts",
    "bench:conditional-deps": "tsx --expose-gc src/runner.ts conditional-deps",
    "bench:diamond-deps": "tsx --expose-gc src/runner.ts diamond-deps",
    "bench:dense-updates": "tsx --expose-gc src/runner.ts dense-updates",
    "bench:batch-operations": "tsx --expose-gc src/runner.ts batch-operations",
    "bench:computed-chains": "tsx --expose-gc src/runner.ts computed-chains",
    "bench:signal-updates": "tsx --expose-gc src/runner.ts signal-updates",
    "bench:effect-triggers": "tsx --expose-gc src/runner.ts effect-triggers",
    "bench:scaling-subscribers": "tsx --expose-gc src/runner.ts scaling-subscribers",
    "bench:wide-fanout": "tsx --expose-gc src/runner.ts wide-fanout",
    "bench:sparse-updates": "tsx --expose-gc src/runner.ts sparse-updates",
    "bench:frameworks": "tsx --expose-gc src/runner.ts frameworks",
    "bench:json": "tsx --expose-gc src/runner.ts",
    "bench:signals": "tsx --expose-gc src/suites/lattice/signals.bench.ts",
    "bench:push-pull": "tsx --expose-gc src/suites/lattice/push-pull.bench.ts",
    "bench:subscription": "tsx --expose-gc src/suites/lattice/subscription-throughput.bench.ts",
    "typecheck": "tsc --noEmit",
    "lint": "eslint . --ext .ts,.tsx",
    "check": "pnpm typecheck && pnpm lint"
  },
  "devDependencies": {
    "@lattice/signals": "workspace:*",
    "@preact/signals-core": "^1.8.0",
    "@reduxjs/toolkit": "^2.8.2",
    "@testing-library/react": "^14.0.0",
    "@types/node": "^20.0.0",
    "@types/react": "^18.3.0",
    "@types/react-dom": "^19.1.6",
    "glob": "^11.0.3",
    "jsdom": "^25.0.0",
    "mitata": "^1.0.34",
    "mobx": "^6.13.6",
    "pinia": "^2.3.0",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "terser": "^5.36.0",
    "tsx": "^4.7.0",
    "vite": "^7.1.1",
    "vitest": "^3.2.4",
    "vue": "^3.5.0",
    "zustand": "^5.0.3"
  },
  "dependencies": {
    "alien-signals": "^2.0.5"
  }
}